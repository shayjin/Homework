
program int X , Y , XY 	;

          begin read X ; Y  := 0 ;
     
	 

EOS

                              read XY, Y ; write XY ;

                              Y  ::= Y + XY ; X  := X - 1 ;
	 
               while 0 < X begin

                              read XY ; write XY ;

                              Y  := Y + XY ; X  := X - 1 ;

		 ;   

	       write Y ;

          end  
      
