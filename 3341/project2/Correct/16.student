program {
    int x, y, z;
    begin {
        x = 5;
        y = 10;
        z = 100;
        while !(x == 0) {
            x = x - 1;
            while !(y == 100) {
                y = y + 1;
                while 0 <= z {
                    z = z - 1;
                }
            }
            write(x);
        }
        write(x);
    }
}